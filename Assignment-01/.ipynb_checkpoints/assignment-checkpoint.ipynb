    "# Area of circle\n",
    "def area_of_circle(r):\n",
    "    pi=3.1416\n",
    "    return pi*r*r\n",
    "\n",
    "# Apply in the Main function\n",
    "print(\"Please provide Width and Height\")\n",
    "radius = float(input())\n",
    "area = area_of_circle(radius)\n",
    "\n",
    "print(\"Area: \", area)"
